{
  "name": "@mavice/aem-vue-editable-components",
  "version": "0.0.0-development",
  "description": "Provides Vue components an integration layer with Adobe Experience Manager Page Editor.",
  "keywords": [
    "spa",
    "aem",
    "vue",
    "adobe"
  ],
  "author": "Mavice LLC",
  "license": "Apache-2.0",
  "homepage": "https://mavice.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/mavicellc/aem-vue-editable-components.git"
  },
  "bugs": {
    "url": "https://github.com/mavicellc/aem-vue-editable-components/issues"
  },
  "engines": {
    "npm": ">=6.14.8",
    "node": ">=10.23.0"
  },
  "main": "dist/aem-vue-editable-components.js",
  "types": "dist/types.d.ts",
  "scripts": {
    "build:production": "cross-env NODE_ENV=production npm run build",
    "build:types": "tsc -p src/tsconfig.types.json",
    "build": "npm run clean && npm run linter && webpack && npm run build:types",
    "clean": "rm -rf dist/",
    "docs": "npm i && npx typedoc --excludePrivate ./src --out ./dist/docs",
    "linter": "eslint . --fix",
    "semantic-release": "semantic-release",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "path-browserify": "^1.0.1"
  },
  "peerDependencies": {
    "vue": "^2.6.11",
    "reflect-metadata": "^0.1.13",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^9.1.2",
    "@adobe/aem-spa-component-mapping": "^1.0.7",
    "@adobe/aem-spa-page-model-manager": "^1.0.0"
  },
  "devDependencies": {
    "@adobe/aem-spa-component-mapping": "~1.0.7",
    "@adobe/aem-spa-page-model-manager": "^1.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@testing-library/dom": "^7.26.3",
    "@types/clone": "^2.0.0",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-router": "^4.5.0",
    "@vue/cli-plugin-typescript": "^4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.0",
    "@vue/cli-service": "^4.5.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^26.1.0",
    "reflect-metadata": "^0.1.13",
    "semantic-release": "^17.4.2",
    "ts-jest": "^26.1.1",
    "ts-loader": "^8.0.2",
    "typedoc": "^0.20.0",
    "typescript": "~3.9.3",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^4.0.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.0.0",
    "webpack-cli": "^4.0.0",
    "webpack-node-externals": "^2.5.2"
  },
  "files": [
    "dist/**/*.{js,ts,vue,map}",
    "!**/{docs,coverage}/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "root": false
  },
  "directories": {
    "test": "tests"
  }
}
